page{
	width: 100%;
	height: 100%;
	background-color: #f7f7f7;
}
.brandListWrap{
	padding-bottom: 30rpx;
}
.brandListWrap_GroupTitle{
	padding: 0 30rpx;
	height: 80rpx;
	line-height: 80rpx;
	font-size: 30rpx;
	font-weight: bold;
}
.brandListWrap_GroupList{
	background-color: #fff;
}
.brandListWrap_GroupList_Item{
	padding: 0 30rpx;
	height: 80rpx;
	line-height: 80rpx;
	align-items: center;
	border-bottom: 2rpx solid #eee;
}
.brandListWrap_GroupList_Item image{
	width: 56rpx;
	height: 56rpx;
	vertical-align: middle;
	margin-right: 40rpx;
}
.brandListWrap_GroupList_Item text{
	font-size: 30rpx;
}
.selectWrap{
	position: fixed;
	right: 20rpx;
	top: 30rpx;
	width: 60rpx;
	z-index: 20;
}
.selectWrap_item{
	text-align: center;
	color: #00aef4;
	font-size: 26rpx;
	padding: 6rpx 0;
	margin-bottom: 4rpx;
}
.posShow{
	z-index: 30;
	width: 80rpx;
	height: 80rpx;
	background-color: rgba(0,0,0,0.8);
	position: fixed;
	top: 50%;
	left: 50%;
	margin-top: -40rpx;
	margin-left: -40rpx;
	font-size: 60rpx;
	text-align: center;
	padding: 10rpx 0;
	color: #fff;
}