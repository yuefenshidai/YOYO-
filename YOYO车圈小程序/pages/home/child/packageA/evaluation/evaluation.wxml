<view class='evaluationWrap'>
	<view class='evaluationWrap_Inner'>
		<view class='carLogo'>
			<image></image>
		</view>
		<view class='Inner_List'>
			<view class='Inner_List_Title'>车辆估价</view>
			<view class='Inner_List_content'>
				<view class='Inner_List_contentItem flex'>
					<text>车辆信息</text>
					<text class='theSecond' bindtap='chooseCarBrand' data-type="{{isChoosed}}">请选择车型</text>
					<image src='../image/entrance.png'></image>
				</view>
				<view class='Inner_List_contentItem flex'>
					<text>注册日期</text>
					<picker class='theSecond' mode = "date" fields="month" bindchange='dateChange' end="{{evaluations.date}}" start="2000-01">
						<text style='display:inline;'>{{evaluations.date}}</text>
					</picker>
				</view>
				<view class='Inner_List_contentItem flex'>
					<text>车辆用途</text>
					<text class='theSecond' bindtap='carUseChange'>{{evaluations.carUse.name}}</text>
					<image src='../image/entrance.png'></image>
				</view>
				<view class='Inner_List_contentItem flex'>
					<text>车辆状况</text>
					<text class='theSecond' bindtap='carStatusChange'>{{evaluations.carStatus.name}}</text>
					<image src='../image/entrance.png'></image>
				</view>
				<view class='Inner_List_contentItem flex'>
					<text>行驶里程</text>
					<input class='theSecond' style='min-width:350rpx;'  placeholder='请输入行驶里程' type='digit'></input>
					<text>万公里</text>
				</view>
				<view class='Inner_List_contentItem flex'>
					<text>购买价格</text>
					<input class='theSecond' style='min-width:350rpx;' placeholder='请输入购车价格' type='digit'></input>
					<text>万元</text>
				</view>
			</view>
		</view>
	</view>
</view>
<view class='offer_now'>
	
</view>